{
  "name": "codeinchq/lib-psr7responsesender",
  "version": "1.0.0",
  "description": "Code Inc. PSR-7 response senders",
  "homepage": "https://github.com/CodeIncHQ/lib-psr7responsesender",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "hansott/psr7-cookies": "^1.0",
    "guzzlehttp/psr7": "^1.4"
  },
  "suggest": {
    "hansott/psr7-cookies": "Allows to add cookies into a PSR-7 response"
  },
  "autoload": {
    "psr-4": {"CodeInc\\Psr7ResponseSender\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Psr7ResponseSender\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
